    <FrameLayout
        android:id="@+id/layFrame"
        android:layout_width="match_parent"
        android:layout_height="0dp"
        android:layout_weight="1">


    </FrameLayout>

    <!--<TextView style="@style/fenge"/>-->

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="52dp"
        android:background="@color/white"
        android:clipChildren="false"
        android:orientation="horizontal"
        android:theme="@style/AppTheme.AppBarOverlay">

        <LinearLayout
            android:id="@+id/id_one_tab"
            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:layout_weight="1"
            android:gravity="center"
            android:orientation="vertical">

            <ImageView
                android:id="@+id/id_one_img"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:src="@mipmap/tab_home_blue" />

            <TextView
                android:id="@+id/id_one_tex"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="3dp"
                android:text="@string/message"
                android:textColor="@color/tab_focuse"
                android:textSize="11sp" />

        </LinearLayout>

        <LinearLayout
            android:id="@+id/id_two_tab"
            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:layout_weight="1"
            android:gravity="center"
            android:orientation="vertical">

            <ImageView
                android:id="@+id/id_two_img"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:src="@mipmap/tab_work" />

            <TextView
                android:id="@+id/id_two_tex"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="3dp"
                android:text="@string/workbench"
                android:textColor="@color/tab_nofocuse"
                android:textSize="11sp" />

        </LinearLayout>

        <LinearLayout
            android:id="@+id/id_three_tab"
            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:layout_weight="1"
            android:gravity="center"
            android:orientation="vertical">

            <ImageView
                android:id="@+id/id_three_img"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:src="@mipmap/tab_mine" />

            <TextView
                android:id="@+id/id_three_text"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="3dp"
                android:text="@string/mine"
                android:textColor="@color/tab_nofocuse"
                android:textSize="11sp" />

        </LinearLayout>


    </LinearLayout>


</LinearLayout>
